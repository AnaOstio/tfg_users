{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch App",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/src/server.ts",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "console": "integratedTerminal",
            "env": {
                "NODE_ENV": "development"
            },
        }
    ]
}